{
  "name": "@paulshryock/logger",
  "version": "0.0.0",
  "description": "PSR-3-compliant logger.",
  "keywords": [
    "psr-3",
    "psr3",
    "log",
    "logger",
    "logging",
    "console"
  ],
  "homepage": "https://github.com/paulshryock/logger",
  "bugs": {
    "url": "https://github.com/paulshryock/logger/issues/new?assignees=&labels=bug&projects=&template=4_report_a_bug.md&title="
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/paulshryock/logger.git"
  },
  "license": "MIT",
  "author": "Paul Shryock (https://github.com/paulshryock)",
  "type": "module",
  "exports": {
    ".": {
      "default": "dist/index.js",
      "types": "dist/index.d.ts"
    }
  },
  "scripts": {
    "precompile": "rm -rf dist",
    "compile": "esbuild src/index.ts --bundle --format=esm --outfile=dist/index.js --platform=node",
    "postcompile": "dts-bundle-generator --no-banner=true --out-file=dist/index.d.ts src/index.ts",
    "test": "jest",
    "typecheck": "tsc"
  },
  "devDependencies": {
    "@babel/core": "7.21.8",
    "@babel/preset-env": "7.21.5",
    "@babel/preset-typescript": "7.21.5",
    "@jest/globals": "29.5.0",
    "@types/node": "20.1.2",
    "babel-jest": "29.5.0",
    "dts-bundle-generator": "8.0.1",
    "esbuild": "0.17.19",
    "jest": "29.5.0",
    "ts-node": "10.9.1",
    "tslib": "2.5.0",
    "typescript": "5.0.4"
  }
}
